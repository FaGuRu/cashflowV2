<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="persistencia">
    <class name="Record" table="record" dynamic-insert="false" dynamic-update="false">
        <id name="id" column="id" type="java.lang.Integer" unsaved-value="null" >
            <generator class="native"></generator>
        </id>

        <property name="type" column="type"/>
        <property name="week_num" column="week_num"/>
        <property name="month" column="month"/>
        <property name="company_name" column="company_name"/>
        <property name="amount" column="amount"/>


    </class>
</hibernate-mapping>